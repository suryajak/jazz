    {
      "Level": 1,
      "Type": "fatal"
    },
    {
      "Level": 2,
      "Type": "error"
    },
    {
      "Level": 3,
      "Type": "warn"
    },
    {
      "Level": 4,
      "Type": "info"
    },
    {
      "Level": 5,
      "Type": "debug"
    },
    {
      "Level": 6,
      "Type": "trace"
    },
    {
      "Level": 7,
      "Type": "verbose"
    },
    {
      "Level": 8,
      "Type": "all"
    }
  ]
}